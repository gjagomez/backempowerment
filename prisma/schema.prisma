generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model cat_preg {
  id         Int       @id @default(autoincrement())
  pregunta   String?   @db.VarChar(455)
  tipo       String?   @db.VarChar(255)
  corr       Int?
  descrip    String?   @db.VarChar(255)
  createat   DateTime  @default(dbgenerated("(curdate())")) @db.Date
  created_at DateTime  @default(now()) @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.DateTime(0)
}

model catemp {
  id           Int       @default(autoincrement())
  idemp        String    @unique(map: "UK_catemp_idemp") @default("") @db.VarChar(255)
  empresa      String?   @db.VarChar(255)
  codemp       Int?
  nombre       String?   @db.VarChar(255)
  puesto       String?   @db.VarChar(255)
  email        String?   @db.VarChar(255)
  codje        Int?
  nomje        String?   @db.VarChar(255)
  puestoje     String?   @db.VarChar(255)
  emailje      String?   @db.VarChar(255)
  departamento String?   @db.VarChar(255)
  region       String?   @db.VarChar(255)
  sucursal     String?   @db.VarChar(255)
  antiguedad   String?   @db.VarChar(255)
  sexo         String?   @db.VarChar(20)
  created_at   DateTime  @default(now()) @db.Timestamp(0)
  updated_at   DateTime? @default(now()) @db.DateTime(0)

  @@id([id, idemp])
}

model empresa {
  id         Int       @id @default(autoincrement())
  idemp      String    @default("") @db.VarChar(245)
  empresa    String?   @db.VarChar(145)
  estado     Int?      @default(0)
  created_at DateTime  @default(now()) @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.DateTime(0)

  @@index([idemp, id], map: "id")
}

model encexist {
  id         Int       @id @default(autoincrement())
  idemp      Int
  emp        String?   @db.VarChar(255)
  encno      Int?
  estado     Int?      @default(0)
  fecin      DateTime? @db.DateTime(0)
  fecfin     DateTime? @db.DateTime(0)
  created_at DateTime  @default(now()) @db.DateTime(0)
  updated_at DateTime? @default(now()) @db.DateTime(0)
}

model cat_put_rep {
  id          Int     @id @default(autoincrement())
  descripcion String? @db.VarChar(255)
  punt        Int?
}

model cat_resp {
  id        Int       @id @default(autoincrement())
  idpreg    Int?
  resp      String?   @db.VarChar(255)
  orden     Int?
  create_at DateTime? @default(dbgenerated("(curdate())")) @db.DateTime(0)
  update_at DateTime? @default(dbgenerated("(curdate())")) @db.DateTime(0)

  @@index([idpreg], map: "IDX_cat_resp_idpreg")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model us_admin {
  id        Int       @id @default(autoincrement())
  idemp     Int?
  codemp    Int?
  email     String?   @db.VarChar(255)
  clave     String?   @db.VarChar(255)
  estado    Int?
  rol       String?   @db.VarChar(255)
  create_at DateTime? @default(dbgenerated("(curdate())")) @db.DateTime(0)
  update_at DateTime? @default(dbgenerated("(curdate())")) @db.DateTime(0)

  @@ignore
}
